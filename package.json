{
  "name": "mflix-api",
  "version": "1.0.0",
  "description": "Express API with Mongoose for MongoDB sample_mflix cluster",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "voyageai": "^0.0.8"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "supertest": "^6.3.3",
    "mongodb-memory-server": "^9.1.5"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": ["/node_modules/"],
    "testMatch": ["**/__tests__/**/*.test.js"],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/config/**"
    ]
  },
  "keywords": [
    "express",
    "mongodb",
    "mongoose",
    "mflix",
    "api"
  ],
  "author": "Rafael Pati√±o",
  "license": "MIT"
}
